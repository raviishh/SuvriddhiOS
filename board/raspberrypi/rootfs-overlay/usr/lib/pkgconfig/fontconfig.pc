prefix=/usr
includedir=${prefix}/include
libdir=${prefix}/lib

sysconfdir=/etc
localstatedir=/var
confdir=/etc/fonts
cachedir=/var/cache/fontconfig

Name: Fontconfig
Description: Font configuration and customization library
Version: 2.17.1
Requires.private: freetype2 >=  21.0.15, expat
Libs: -L${libdir} -lfontconfig
Libs.private: -pthread -lm
Cflags: -I${includedir}
